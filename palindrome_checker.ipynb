    "def isPalindrome(str):\n",
    "\n",
    "\n",
    "\tfor i in range(0, int(len(str)/2)):\n",
    "\t\tif str[i] != str[len(str)-i-1]:\n",
    "\t\t\treturn False\n",
    "\treturn True\n",
    "\n",
    "# main function\n",
    "s =input((\"Enter a string:\"))\n",
    "ans = isPalindrome(s)\n",
    "\n",
    "if (ans):\n",
    "\tprint(\"true\")\n",
    "else:\n",
    "\tprint(\"false\")\n"
   ]
